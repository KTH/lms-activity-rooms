    "cuid": "^2.1.8",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fast-csv": "^4.3.6",
    "got": "^11.6.0",
    "jszip": "^3.5.0",
    "memoizee": "^0.4.14",
    "skog": "^1.0.0-alpha.1"
  },
  "devDependencies": {
    "@kth/eslint-config-kth": "^1.0.10",
    "ava": "^3.15.0",
    "colors": "^1.4.0",
    "eslint": "^7.29.0",
    "nock": "^13.0.7",
    "nodemon": "^2.0.4",
    "prettier": "^2.3.2"
  }
